{
   "workers": [
      {
         "type": "router",
         "options": {
            "pythonpath": [
               ".."
            ]
         },
         "realms": [
            {
               "name": "realm_limited",
               "roles": [
                  {
                     "name": "anonymous",
                     "permissions": [
                        {
                           "uri": "test.no_publish.*",
                           "publish": false,
                           "subscribe": true,
                           "call": true,
                           "register": true
                        }
                     ]
                  },
                  {
                     "name": "anonymous",
                     "permissions": [
                        {
                           "uri": "test.no_subscribe.*",
                           "publish": true,
                           "subscribe": false,
                           "call": true,
                           "register": true
                        }
                     ]
                  },
                  {
                     "name": "anonymous",
                     "permissions": [
                        {
                           "uri": "test.no_call.*",
                           "publish": true,
                           "subscribe": true,
                           "call": false,
                           "register": true
                        }
                     ]
                  },
                  {
                     "name": "anonymous",
                     "permissions": [
                        {
                           "uri": "test.no_register.*",
                           "publish": true,
                           "subscribe": true,
                           "call": true,
                           "register": false
                        }
                     ]
                  }
               ]
            },
            {
               "name": "realm1",
               "roles": [
                  {
                     "name": "anonymous",
                     "permissions": [
                        {
                           "uri": "*",
                           "publish": true,
                           "subscribe": true,
                           "call": true,
                           "register": true
                        }
                     ]
                  }
               ],
"store": {
   "type": "memory",
   "event-history": [
      {
         "uri": "com.historized.event",
         "limit": 10000
      }
   ]
}
            }
         ],
         "transports": [
            {
               "type": "web",
               "endpoint": {
                  "type": "tcp",
                  "port": 8080
               },
               "paths": {
                  "ws": {
                     "type": "websocket"
                  }
               }
            }
         ]
      }
   ]
}